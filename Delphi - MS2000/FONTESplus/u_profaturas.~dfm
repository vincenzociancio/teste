  object qrExportadores: TQuery
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'SELECT E.Codigo, E.Razao_Social,E.Pais'
      
        'FROM Parametros AS P INNER JOIN Exportadores AS E ON P.Filial = ' +
        'E.Filial AND P.Empresa = E.Empresa'
