  object qrProcessos: TQuery
    CachedUpdates = True
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      
        'SELECT PRO.Empresa,PRO.status, PRO.Filial,Codigo,Data, Centro_Cu' +
        'sto, vinc_processo, Tipo, Importador, Codigo_Cliente,Pag_proporc' +
        'ional,Tipo_Declaracao,Contrato,Local_Inventario, Responsavel_Emp' +
        'resaC, Responsavel_Empresa, PRO.Numero_RTC'
      'FROM Processos AS PRO '
      
        'INNER JOIN Parametros AS PAR  ON (PAR.Filial = PRO.Filial) AND (' +
        'PAR.Empresa = PRO.Empresa)'
      'WHERE PRO.Codigo=:Processo'
      'ORDER BY PRO.Codigo;')
    Left = 528
    Top = 48
    ParamData = <
      item
        DataType = ftString
