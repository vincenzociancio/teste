  object qrImport: TQuery
    AfterScroll = qrImportAfterScroll
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'SELECT *'
      'FROM Importadores'
      'WHERE Ativo = '#39'1'#39' AND Empresa = :emp AND Filial = :filial'
      'ORDER BY Razao_Social')
    Left = 664
    Top = 112
    ParamData = <
      item
        DataType = ftString
        Name = 'emp'
        ParamType = ptUnknown
        Value = '1'
