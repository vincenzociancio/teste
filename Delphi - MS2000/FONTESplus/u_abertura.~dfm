  object qryNumerarioObrigatorio: TQuery
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'select '
      ' FK_Tipo_Processo,'
      ' FK_Tipo_Declaracao,'
      ' FK_Tipo_Numerario'
      'from'
      ' Tipos_Numerario_Obrigatorio'
      'where NOT EXISTS('
      'select '
      ' Numerarios_Processos.Numerario'
      'from '
      ' Numerarios_Processos '
      'where '
      
