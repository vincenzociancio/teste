  object qrHeader: TQuery
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'select distinct'
      'p.Codigo, p.NR_DECLARACAO_IMP as '#39'dctoImportacao'#39','
      
        'SUBSTRING(p.DT_REGISTRO_DI, 5, 4)+'#39'-'#39'+SUBSTRING(p.DT_REGISTRO_DI' +
        ', 3, 2)+'#39'-'#39'+SUBSTRING(p.DT_REGISTRO_DI, 1, 2) as '#39'dataDctoImport' +
        'acao'#39','
      
        'cast(CAST(p.DT_DESEMBARACO AS DATE) as varchar(10)) as '#39'data_des' +
        'embaraco'#39','
      'p.Valor_FOB, p.Valor_CIF,'
      'f.Codigo as '#39'nfatura'#39','
      'f.Moeda,f.Peso_Total_Acertado as '#39'Peso_Liquido'#39','
      'f.Peso_Total as '#39'peso_bruto'#39','
      'case f.cobertura_cambial'
      #9'when 1 then '#39'Y'#39
      #9'else '#39'N'#39
      'end as '#39'COBERTURA_CAMBIAL'#39','
      'case f.Moeda'
      #9'when '#39'220'#39' then '#39'USD'#39
      #9'when '#39'978'#39' then '#39'EUR'#39
      #9'when '#39'790'#39' then '#39'BRL'#39
      #9'else '#39'OUT'#39' '
      'end as '#39'Cod_Moeda'#39','#9
      'CP.URF_Despacho as '#39'UFDesembaraco'#39', cp.Valor_Seguro, '
      'TIF.CD_APLICACAO_MERC AS '#39'NATUREZA_OPERACAO'#39','
      'case '
      
        'when (Expo.CGC_CPF IS null) OR (Expo.CGC_CPF = '#39#39') then IMP.CNPJ' +
        '_CPF_COMPLETO'
      'else Expo.CGC_CPF'
      'end AS '#39'CNPJ_DEST'#39','
      'IMP.CNPJ_CPF_COMPLETO'
      'from processos as p '
      'inner join Faturas as f on (p.Codigo = f.processo)'
      
        'inner join Conhecimento_Processo as CP on (P.Codigo = CP.Process' +
        'o)'
      
        'inner join Processos_Taxas_Conversao as PTC on (p.Codigo=PTC.Pro' +
        'cesso and PTC.Moeda = f.Moeda)'
      
        'inner join Tributacao_Item_Fatura as TIF on (f.Codigo = TIF.Fatu' +
        'ra)'
      
        'inner join ItensFaturas as ItF on (f.Codigo = itf.Fatura and TIF' +
        '.Sequencial_Item = itf.Sequencial)'
      
        'inner join Exportadores as Expo on (f.Exportador = Expo.Codigo) ' +
        'AND (f.Filial = Expo.Filial) '
      'INNER JOIN Importadores AS IMP ON (p.Importador = IMP.Codigo)'
      'where '
      'p.Codigo =:pProcesso and f.Codigo =:pFatura'
      'order by f.Codigo'
      ' ')
    Left = 376
    ParamData = <
      item
        DataType = ftString
        Name = 'pProcesso'
        ParamType = ptInput
      end
      item
        DataType = ftString
        Name = 'pFatura'
        ParamType = ptInput
      end>
