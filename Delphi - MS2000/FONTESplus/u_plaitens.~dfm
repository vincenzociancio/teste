    SQL.Strings = (
      'select SUM([ITF].[Base_Calc_II]*[PTC].[taxa_conversao])'
      'from ItensFaturas as itf'
      
        'INNER JOIN Faturas ON itf.Fatura = Faturas.Codigo AND itf.Proces' +
        'so = Faturas.Processo'
      #9'AND Faturas.Processo = :p1'
      
