  object qryColaboradores: TQuery
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'select '
      ' Usuario'
      'from'
      ' Usuarios'
      'where'
      
        'not Exists ( select colaborador from Controle_Hierarquico_Colabo' +
        'rador where colaborador = Usuario )'
      'Order by '
      'Usuario')
    Left = 225
    Top = 72
  end
