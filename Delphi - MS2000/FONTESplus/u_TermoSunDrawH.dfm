  object qrLocProc: TQuery
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      
        'SELECT P.Codigo, P.Data AS DataProc, I.Razao_Social, I.CNPJ_CPF_' +
        'COMPLETO, I.Endereco, I.Numero, I.Complemento, I.Bairro, I.Cidad' +
        'e, I.UF, I.CEP, AC.Numero, CP.Numerodoccarga, CP.Data, CP.Local,' +
        ' TP.DESCRICAO, CP.Embarcacao, CP.Data_Chegada_URF_Cheg, PTC.Taxa' +
        '_conversao, CP.Peso_Bruto, CP.Peso_Liquido, CP.Frete_Prepaid, P.' +
        'NR_DECLARACAO_IMP'
      
        'FROM ((((Processos AS P INNER JOIN Importadores AS I ON (P.Empre' +
        'sa = I.Empresa) AND (P.Filial = I.Filial) AND (P.Importador = I.' +
        'Codigo)) INNER JOIN Conhecimento_Processo AS CP ON (P.Codigo = C' +
        'P.Processo) AND (P.Filial = CP.Filial) AND (P.Empresa = CP.Empre' +
        'sa)) LEFT JOIN TAB_PAIS AS TP ON CP.Pais_Procedencia = TP.CODIGO' +
        ') LEFT JOIN Processos_Taxas_Conversao AS PTC ON (P.Codigo = PTC.' +
        'Processo) AND (P.Filial = PTC.Filial) AND (P.Empresa = PTC.Empre' +
        'sa)) LEFT JOIN (Atos_Importador AS AI LEFT JOIN Atos_concessorio' +
        's AS AC ON (AI.Numero = AC.Numero) AND (AI.Filial = AC.Filial) A' +
        'ND (AI.Empresa = AC.Empresa)) ON (P.Importador = AI.Importador) ' +
        'AND (P.Filial = AI.Filial) AND (P.Empresa = AI.Empresa)'
      'WHERE P.Codigo = :processo  AND PTC.Moeda = "220"')
    Left = 352
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'processo'
        ParamType = ptUnknown
      end>
