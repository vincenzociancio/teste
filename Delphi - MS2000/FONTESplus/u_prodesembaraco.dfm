  object qrsomaqtd: TQuery
    AutoCalcFields = False
    DatabaseName = 'DBNMSCOMEX'
    SQL.Strings = (
      'SELECT Sum(ItensFaturas.Quantidade) AS SomaDeQuantidade'
      
        'FROM ItensFaturas INNER JOIN Tributacao_Item_Fatura ON (ItensFat' +
        'uras.Empresa = Tributacao_Item_Fatura.Empresa) AND (ItensFaturas' +
        '.Filial = Tributacao_Item_Fatura.Filial) AND (ItensFaturas.Proce' +
        'sso = Tributacao_Item_Fatura.Processo) AND (ItensFaturas.Fatura ' +
        '= Tributacao_Item_Fatura.Fatura) AND (ItensFaturas.Sequencial = ' +
        'Tributacao_Item_Fatura.Sequencial_Item)'
      
        'WHERE (((Tributacao_Item_Fatura.Regime_Tributacao_II) Like :qreg' +
        'ii) AND ((Tributacao_Item_Fatura.Regime_Tributacao_IPI) Like :qr' +
        'egipi))'
      'GROUP BY Tributacao_Item_Fatura.Processo'
      'HAVING (((Tributacao_Item_Fatura.Processo)=:qpro));')
    Left = 680
    Top = 1
    ParamData = <
      item
        DataType = ftString
        Name = 'qregii'
        ParamType = ptUnknown
        Value = '5'
      end
      item
        DataType = ftString
        Name = 'qregipi'
        ParamType = ptUnknown
        Value = '5'
      end
      item
        DataType = ftString
        Name = 'qpro'
        ParamType = ptUnknown
        Value = '02354/02'
      end>
