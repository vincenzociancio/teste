@model MS2000WEB.Models.AcessoGrupos

@{
    ViewBag.Title = "Delete";
}

<ul class="breadcrumb">
    <li><a href="#">Home</a> <span class="divider">></span></li>
    <li><a href="#">Sistema</a> <span class="divider">></span></li>
    <li><a href="#">Grupos de Acesso</a> <span class="divider">></span></li>
    <li class="active">Deletar</li>
</ul>

<h4>Grupos de Acesso do Sistema</h4>

@using (Html.BeginForm(null, null, FormMethod.Post, new { @class = "" }))
{
    <div class="alert-message block-message info">        
        <h3>Informação!</h3>
        <br />
        <p>Tem certeza de que deseja excluir este registro?</p>
        <br />
        <hr />
        <div class="alert-actions">
            <input type="submit" class="btn btn-danger" name="Delete" value="Deletar" />
            @Html.ActionLink("Cancelar", "Index", null, new { @class = "btn" })
        </div>
    </div>
}



